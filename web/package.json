{
	"name": "web",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"ng": "ng",
		"dev": "ng serve",
		"build": "ng build --configuration production",
		"watch": "ng build --watch --configuration development",
		"test:eslint": "eslint 'src/**/*.ts' --cache --cache-location node_modules/.cache/.eslintcache --cache-strategy content",
		"test:stylelint": "stylelint \"src/**/*.{css,scss}\"",
		"test:format": "prettier --write --ignore-unknown .",
		"test:ng": "ng test --watch false",
		"test": "pnpm run /^test:/"
	},
	"private": true,
	"packageManager": "pnpm@10.24.0",
	"dependencies": {
